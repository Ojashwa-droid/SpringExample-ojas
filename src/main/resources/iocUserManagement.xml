<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- bean definitions here -->
    <bean id="userDataProvider" class="com.ojas.Ioc.UserDatabaseProvider"/>

    <bean id="newDataProvider" class="com.ojas.Ioc.NewDatabaseProvider"/>

    <bean id="userManagerWithUserDataProvider" class="com.ojas.Ioc.UserManager">
        <constructor-arg ref="userDataProvider"/>
    </bean>

    <bean id="userManagerWithNewDataProvider" class="com.ojas.Ioc.UserManager">
        <constructor-arg ref="newDataProvider"/>
    </bean>

</beans>